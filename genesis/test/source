	    log("  Iteration " + toliteral(i));
	log("  Final value " + toliteral(i));
	for i in (#[["foo", "bar"], [3, 'foo], [~baz, [3, 4]]])
	    log("  Iteration " + toliteral(i));
	log("  Final value " + toliteral(i));
